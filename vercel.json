{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "RESEND_API_KEY": "@resend-api-key"
  },
  "analytics": {
    "enable": true
  },
  "speedInsights": {
    "enable": true
  }
} 